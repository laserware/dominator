{
  "name": "@laserware/dominator",
  "version": "2.3.2",
  "description": "Simplify DOM usage.",
  "browser": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laserware/dominator.git"
  },
  "bugs": "https://github.com/laserware/dominator/issues",
  "author": "Mike Rourke <mike.w.rourke@gmail.com>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "dom"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup && cd dist && npx prettier --write index.d.ts",
    "prepublishOnly": "npm run build",
    "preversion": "npm run eslint && npm run typecheck",
    "version": "npm run prettier && git add -A src",
    "postversion": "git push && git push --tags",
    "eslint": "eslint --cache src",
    "prettier": "prettier --cache --write src",
    "prettier:ci": "prettier --check src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@laserware/arcade": "^2.4.0"
  },
  "devDependencies": {
    "@laserware/eslint-config": "^2.0.0",
    "@rushstack/eslint-patch": "^1.7.2",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "prettier": {
    "trailingComma": "all"
  }
}
